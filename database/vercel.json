{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)",
    "dest" : "api/server.js"
      }
    ]
  }